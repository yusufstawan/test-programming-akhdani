{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "author": "yusufstawan",
  "license": "MIT",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "npm run prisma:generate && tsc",
    "start": "node dist/index.js",
    "prisma:generate": "npx prisma generate --config ./prisma.config.ts",
    "prisma:migrate": "npx prisma migrate dev --config ./prisma.config.ts",
    "prisma:studio": "npx prisma studio --config ./prisma.config.ts",
    "prisma:reset": "npx prisma migrate reset",
    "db:seed": "npx prisma db seed --config ./prisma.config.ts",
    "seed": "tsx prisma/seed.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "keywords": [],
  "dependencies": {
    "@prisma/adapter-pg": "^7.0.1",
    "@prisma/client": "^7.0.1",
    "@perdinpro/types": "workspace:*",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.15.3",
    "@types/pg": "^8.15.6",
    "prisma": "^7.0.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
